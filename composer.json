{
    "name": "fintecture/payment",
    "description": "Fintecture Payment",
    "keywords": ["payment","bank"],
    "homepage": "https://github.com/Fintecture/magento",
    "support": {
        "email": "contact@fintecture.com"
    },
    "type": "magento2-module",
    "version": "1.5.0",
    "license": [
        "GPL-3.0"
    ],
    "require": {
        "php": ">=7.3",
        "ext-json": "*",
        "ext-openssl": "*",
        "magento/framework": ">=100",
        "magento/module-developer": ">=100"
    },
    "require-dev": {
        "phpstan/phpstan": "^1",
        "bitexpert/phpstan-magento": "^0.23.0",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/extension-installer": "^1.1"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    ],
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Fintecture\\Payment\\": "."
        }
    },
    "config": {
        "allow-plugins": {
            "magento/composer-dependency-version-audit-plugin": true,
            "phpstan/extension-installer": true
        }
    }
}
