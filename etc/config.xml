<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <payment>
            <fintecture>
                <model>Fintecture\Payment\Model\Fintecture</model>
                <active>0</active>
                <title>Fintecture</title>
                <allowspecific>1</allowspecific>
                <specificcountry>FR</specificcountry>
                <environment>sandbox</environment>
                <!-- <fintecture_app_id_sandbox></fintecture_app_id_sandbox> -->
                <fintecture_app_secret_sandbox backend_model="Magento\Config\Model\Config\Backend\Encrypted"/>
                <!-- <custom_file_upload_sandbox>app_privateKey.pem</custom_file_upload_sandbox> -->
                <!-- <fintecture_app_id_production></fintecture_app_id_production> -->
                <fintecture_app_secret_production backend_model="Magento\Config\Model\Config\Backend\Encrypted"/>
                <payment_us_fintecture_general_specificcountry>FR</payment_us_fintecture_general_specificcountry>
                <payment_us_fintecture_general_allowspecific>1</payment_us_fintecture_general_allowspecific>
            </fintecture>
        </payment>
    </default>
</config>
